"apiv2domnam{{this.Name}}": {
  "Type" : "AWS::ApiGatewayV2::DomainName",
  "Properties" : {

      "DomainName": "{{this.DomainName}}",

      {{#if this.DomainNameConfigurations}}
        "DomainNameConfigurations": [
        {{#each this.DomainNameConfigurations}}
          "{{this}}"{{comma}}
        {{/each}}
        ],
      {{/if}}
      {{#if this.MutualTlsAuthentication}}
        "MutualTlsAuthentication": { {{{inject this.MutualTlsAuthentication}}} },
      {{/if}}

      {{> tags_map}}
    }
}
