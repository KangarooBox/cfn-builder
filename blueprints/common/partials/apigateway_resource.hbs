{{!--
  Creates an API Gateway Resource

  https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/aws-resource-apigateway-resource.html
 --}}
"apigwresc{{this.Name}}": {
  "Type": "AWS::ApiGateway::Resource",
  "Properties": {
    "ParentId": { "Fn::GetAtt": ["apigwrest{{this.ParentId}}", "RootResourceId"] },
    "PathPart": "{{this.PathPart}}",
    "RestApiId": { "Ref": "apigwrest{{this.RestApiId}}" }
  }
}
