{{!--
  Uses an AWS Lambda (Lambda) function that evaluates configuration items to assess whether your
  AWS resources comply with your specified configurations.

  http://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/aws-resource-config-configrule.html
 --}}
"dc{{this.Name}}" : {
  "Type" : "AWS::Config::DeliveryChannel",
  "Properties" : {
    "Name" : "{{this.Name}}",
    {{#if this.ConfigSnapshotDeliveryProperties}}
    "ConfigSnapshotDeliveryProperties" : { "DeliveryFrequency" : "{{this.ConfigSnapshotDeliveryProperties.DeliveryFrequency}}" },
    {{~/if}}
    {{#if this.S3KeyPrefix}}"S3KeyPrefix" : "{{this.S3KeyPrefix}}",{{~/if}}
    {{#if this.SnsTopicARN}}"SnsTopicARN" : "{{this.SnsTopicARN}}",{{~/if}}
    "S3BucketName" : "{{this.S3BucketName}}"
  }
}
