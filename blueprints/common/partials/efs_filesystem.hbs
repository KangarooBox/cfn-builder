{{!--
  Creates a new, empty file system in Amazon Elastic File System (Amazon EFS).

  http://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/aws-resource-efs-filesystem.html
 --}}
"efsfs{{this.Name}}":{
  "Type": "AWS::EFS::FileSystem",
  "Properties": {
    {{#if this.Encrypted}}"Encrypted": "{{this.Encrypted}}",{{/if}}
    {{#if this.KmsKeyId}}"KmsKeyId": "{{this.KmsKeyId}}",{{/if}}
    {{#if this.PerformanceMode}}"PerformanceMode": "{{this.PerformanceMode}}",{{/if}}
    {{> tags PropertyID="FileSystemTags"}}
  }
}
