{{!--
  Creates an AppStream Stack Fleet Association
https://docs.aws.amazon.com/AWSCloudFormation/latest/UserGuide/aws-resource-appstream-stackfleetassociation.html
--}}

"appstream{{sanitize this.Name ''}}":{
  "Type" : "AWS::AppStream::StackFleetAssociation",
  "Properties" : {
      "FleetName" : {"Ref": "appstream{{sanitize this.FleetName ''}}"},
      "StackName" : {"Ref": "appstream{{sanitize this.StackName ''}}"}
    }
}
